{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        // "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": 0,
        "no-restricted-syntax": 0,
        "no-plusplus": 0,
        "guard-for-in": 0,
        "max-len": 0,
        "no-const-assign": 0
    }
}
